
<%- include("../includes/user-header", {user:userInfo}); -%>
      <!-- TOP Nav Bar END -->
      <!-- Page Content  -->
      <div id="content-page" class="content-page">
         <div class="container-fluid">
            <div class="row">
               <div class="col-lg-12">
                  <div class="iq-card">
                     <div class="iq-card-header d-flex justify-content-between align-items-center">
                        <div class="iq-header-title">
                           <h4 class="card-title">Your Tracks</h4>
                        </div>
                        
                        <div id="feature-album-slick-arrow" class="slick-aerrow-block"></div>
                        <div class="iq-card-header-toolbar d-flex align-items-center">
                           <a href="/submit-song" class="btn btn-primary">Add New Song</a>
                        </div>
                     </div>
                     <div class="iq-card-body">
                        <ul class="list-unstyled row  iq-box-hover mb-5">
                          
                           <% userInfo.tracks.forEach(function(track){ %>
                              <li class="col-lg-2 iq-music-box">
                                 <div class="iq-card mb-0">
                                    <div class="iq-card-body p-0">
                                       <div class="iq-thumb">
                                          <div class="iq-music-overlay"></div>
                                          <a href="music-player.html">
                                             <img src="<%=track.albumart_link%>" class="img-border-radius img-fluid w-100" alt="">
                                          </a>
                                          <div class="overlay-music-icon">
                                             <a href="music-player.html">
                                                <i class="las la-play-circle"></i>
                                             </a>
                                          </div>
                                       </div>
                                       <div class="feature-list text-center">
                                          <h6 class="font-weight-600 mb-0"><%=track.track_title%></h6>
                                          <p class="mb-0"><%= userInfo.artistname%></p>
                                       </div>
                                    </div>
                                 </div>
                              </li>
                             <% }) %>
                           
                         
                           
                        </ul>
                     </div>
                  </div>
               </div>
               
            </div>
         </div>
      </div>
   </div>
   <!-- Wrapper END -->
   <%- include("../includes/user-footer"); -%>